prefix=/usr
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include
pluginsdir=/usr/lib/plymouth/
themesdir=/usr/share/plymouth/themes/
confdir=/etc/plymouth/
policydir=/usr/share/plymouth/

Name: Plymouth
Description: Utility Library for Boot Splash Plugins
Version: 22.02.122
Libs: -L${libdir} -lply-splash-core -lply -ldl
Cflags: -I${includedir}/plymouth-1/ply -I${includedir}/plymouth-1/ply-splash-core
